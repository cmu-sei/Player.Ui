{
  "name": "player.ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:init": "docker network create identity --driver=overlay",
    "docker:stack": "npm run docker:stack:api && npm run docker:stack:identity",
    "docker:stack:proxy": "docker stack deploy -c ./docker/docker-compose.yml -c ./docker/compose/api.yml -c ./docker/api-proxy.yml player_stack",
    "docker:stack:api": "docker stack deploy -c ./docker/docker-compose.yml -c ./docker/compose/api.yml player_stack",
    "docker:stack:identity": "docker stack deploy -c ./docker/docker-compose.yml -c ./docker/compose/identity.yml identity_stack",
    "docker:destroy": "docker stack rm player_stack identity_stack",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "swagger:gen-win": "docker run --rm -v %CD%:/local openapitools/openapi-generator-cli:v4.3.0 generate -i http://host.docker.internal:4300/swagger/v1/swagger.json?format=openapi -g typescript-angular -o /local/src/app/generated/player-api --additional-properties ngVersion=9.1 --additional-properties useRxJS6=true --additional-properties modelPropertyNaming=original --type-mappings=DateTime=Date --skip-validate-spec"
  },
  "scriptComments": {
    "docker:stack": "// Runs the entire player stack",
    "docker:stack:proxy": "// Runs the entire caster player with proxy settings",
    "docker:destroy": "// Destroys the docker player, data retained"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^11.2.0",
    "@angular-devkit/schematics": "^11.2.0",
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/localize": "^11.2.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@aspnet/signalr": "^1.1.4",
    "@auth0/angular-jwt": "^4.1.2",
    "@datorama/akita": "^4.23.2",
    "@datorama/akita-ng-router-store": "^5.1.4",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@schematics/angular": "^9.1.7",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "ng-socket-io": "^0.2.4",
    "ngx-clipboard": "^13.0.1",
    "oidc-client": "^1.10.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "rxjs-tslint": "^0.1.8",
    "tether": "^1.4.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@datorama/akita-ngdevtools": "^4.0.0",
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.0.7"
  }
}
